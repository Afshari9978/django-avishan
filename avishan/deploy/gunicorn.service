[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=<<USERNAME>>
Group=www-data
WorkingDirectory=<</home/sammy/myproject>>
ExecStart=<</home/sammy/myproject>>/venv/bin/gunicorn --timeout 90 --access-logfile - --workers 3 --bind unix:<</home/sammy/myproject>>/gunicorn.sock <<myproject>>.wsgi:application

[Install]
WantedBy=multi-user.target